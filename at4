#include <iostream>
using namespace std;
int main(){
    
    int m1 [3][3]={{3,1,2},{2,2,3},{3,3,2}};
    int m2 [3][3]={{3,1,2},{2,2,3},{3,3,2}};
    int igual;
    
    
    for (int i=0;i<3;i++){
        for (int j=0;j<3;j++){
            if(m1[i][j]==m2[i][j]){
                igual=m2[i][j];
                cout<<"linha: "<<i+1<<' '<<"coluna: "<<j+1<<endl;
                cout<<"numero: "<<igual<<endl;
                
                }
            }
        }
        
    return 0;
